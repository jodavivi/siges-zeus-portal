{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.telcomdataperu.app.Portal",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.telcomdataperu.app.Portal.view.Root",
			"type": "XML",
			"async": true,
			"id": "Root"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.telcomdataperu.app.Portal.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.telcomdataperu.app.Portal.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"pattern": "",
				"name": "appInicio",
				"target": "home"
		},
		{
			"pattern": "usuario",
			"name": "appUsuario",
			"target": "usuario"
		},
		{
			"pattern": "usuario/:id:/editar",
			"name": "appUsuario2",
			"target": "usuario"
		},
		{
			"pattern": "usuario/:id:/detalle",
			"name": "appUsuario3",
			"target": "usuario"
		},
		{
			"pattern": "usuario/registro",
			"name": "appUsuario4",
			"target": "usuario"
		},
		{
			"pattern": "maestra:?query:",
			"name": "appMaestra",
			"target": "maestra"
		},
		{
			"pattern": "maestra/detalle/:id:",
			"name": "appMaestra2",
			"target": "maestra"
		},
		{
			"pattern": "maestra/detalle/:id:/parametro/:id:",
			"name": "appMaestra3",
			"target": "maestra"
		},
		{
			"pattern": "acceso:?query:",
			"name": "appAcceso",
			"target": "targetAcceso"
		},
		{
			"pattern": "acceso/aplicaciones:?query:",
			"name": "appAcceso2",
			"target": "targetAcceso"
		},
		{
			"pattern": "acceso/grupos:?query:",
			"name": "appAcceso3",
			"target": "targetAcceso"
		},
		{
			"pattern": "acceso/permisos:?query:",
			"name": "appAcceso4",
			"target": "targetAcceso"
		},
		{
			"pattern": "auditoria:?query:",
			"name": "appAuditoria",
			"target": "targetAuditoria"
		} ,
		{
			"pattern": "producto-categoria:?query:",
			"name": "appProductoCategoria",
			"target": "targetProductoCategoria"
		},
		{
			"pattern": "producto-marca:?query:",
			"name": "appProductoMarca",
			"target": "targetProductoMarca"
		},
		{
			"pattern": "producto:?query:",
			"name": "appProducto",
			"target": "targetProducto"
		},
		{
			"pattern": "cliente:?query:",
			"name": "appCliente",
			"target": "targetCliente"
		}],
			"targets": {
				"home": {
					"transition": "slide",
					"viewName": "Aplicaciones",
					"viewLevel": 1
				  },
				  "usuario": {
					"transition": "slide",
					"viewName": "UsuariosComponent",
					"viewId": "UsuariosComponent",
					"viewLevel": 2
				  },
				  "maestra": {
					"transition": "slide",
					"viewName": "MaestraComponent",
					"viewId": "MaestraComponent",
					"viewLevel": 3
				  },
				  "targetAcceso": {
					"transition": "slide",
					"viewName": "AccesoComponent",
					"viewId": "AccesoComponent",
					"viewLevel": 3
				  },
				  "targetAuditoria": {
					"transition": "slide",
					"viewName": "AuditoriaComponent",
					"viewId": "AuditoriaComponent",
					"viewLevel": 4
				  },
				  "targetProductoCategoria": {
					"transition": "slide",
					"viewName": "ProductoCategoriaComponent",
					"viewId": "ProductoCategoriaComponent",
					"viewLevel": 5
				  },
				  "targetProductoMarca": {
					"transition": "slide",
					"viewName": "ProductoMarcaComponent",
					"viewId": "ProductoMarcaComponent",
					"viewLevel": 6
				  },
				  "targetProducto": {
					"transition": "slide",
					"viewName": "ProductoComponent",
					"viewId": "ProductoComponent",
					"viewLevel": 7
				  },
				  "targetCliente": {
					"transition": "slide",
					"viewName": "ClienteComponent",
					"viewId": "ClienteComponent",
					"viewLevel": 8
				  }
			}
		}
	}
}