{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.telcomdataperu.app.Maestra",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.telcomdataperu.app.Maestra.view.Home",
			"type": "XML",
			"async": true,
			"id": "Home"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.telcomdataperu.app.Maestra.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.telcomdataperu.app.Maestra.view",
				"controlAggregation": "pages",
				"controlId": "idSplitContainer",
				"clearControlAggregation": false
			},
			"routes": [ {
					"pattern": "maestra",
					"name": "maestraRoute",
					"target": [
						"TargetHome",
						"targetNotFound"
					] 
			  },{
					"name": "RouteHomeDetalle",
					"pattern": "maestra/detalle/{maestroId}",
					"target": [
						"TargetHome",
						"targetDetail"
					]
				},{
					"name": "RouteHomeAddParameter",
					"pattern": "maestra/detalle/{maestroId}/parametro",
					"target": [
						"TargetHome",
						"targetAddParametro"
					]
				},{
					"name": "RouteHomeEditParameter",
					"pattern": "maestra/detalle/{maestroId}/parametro/{parametroId}",
					"target": [
						"TargetHome",
						"targetEditParametro"
					]
				}
			],
			"targets": { 
				"TargetHome": {
					"transition": "slide",
					"viewName": "Maestra",
					"controlAggregation": "masterPages",
					"viewLevel": 0
				},
				"targetNotFound": {
					"transition": "slide",
					"viewName": "NotFound",
					"controlAggregation": "detailPages",
					"viewLevel": 1
				},
				"targetDetail": {
					"transition": "slide",
					"viewName": "Detail",
					"controlAggregation": "detailPages",
					"viewLevel": 2
				},
				"targetAddParametro": {
					"transition": "slide",
					"viewName": "ParametroRegistro",
					"controlAggregation": "detailPages",
					"viewLevel": 3
				},
				"targetEditParametro": {
					"transition": "slide",
					"viewName": "ParametroEditar",
					"controlAggregation": "detailPages",
					"viewLevel": 3
				}

			}
		}
	}
}