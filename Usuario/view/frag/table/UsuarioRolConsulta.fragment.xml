<core:FragmentDefinition
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" 
	displayBlock="true" 
	xmlns="sap.m" 
	xmlns:f="sap.ui.layout.form" 
	xmlns:core="sap.ui.core" 
	xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified"
	xmlns:ui="sap.ui.table"> 
	 
			 <VBox class="sapUiSmallMargin">
				 <List
		id="ListaRoles"
		items="{ path : 'modelAcceso>/aListaRolesUsuarioActual' , sorter: { path: 'Id', ascending: true }}"
		>
		<headerToolbar>
			<OverflowToolbar>
				<content>
					<Title text="Roles Asignados" level="H2"/> 
				</content>
			</OverflowToolbar>
		</headerToolbar>
		<StandardListItem
			title="{modelAcceso>Rol}"
			description="{modelAcceso>CodRol}" 
			iconDensityAware="false"
			iconInset="false"   />
	</List>
			</VBox>
		 

</core:FragmentDefinition>